---
import Container from "./Container.astro"
import LogoO from "./logoO.astro"

---
<Container>
    <div class="relative flex flex-col items-center md:flex-row">
        <div class="row items-center py-5 md:pr-16 md:w-6/12 md:py-10">
            <div class="text-center md:text-left space-y-3 flex flex-row items-center  md:ml-10">
                <div class="spinner">
                    <LogoO/>
                </div>
                <div>
                <h2 class="text-4xl font-medium leading-none sm:text-6xl bebas-neue-regular ml-2">Timeline</h2></div>
            </div>
        </div>
        </div>

        <div>
            <table>
                <thead>
                    <tr>
                        <th class="font-sansroboto-condensed bebas-neue-regular text-lg table-1 border-solid border-[1px] border-white text-white bg-google_red">Session</th>
                        <th class="font-sansroboto-condensed bebas-neue-regular text-lg border-solid border-[1px] border-white text-white bg-google_red">Duration</th>
                        <th class="font-sansroboto-condensed bebas-neue-regular text-lg border-solid border-[1px] border-white text-white bg-google_red">Timing</th>
                    </tr>
                </thead>
                <tbody>
                    <!-- Repeat rows as needed -->
                    <tr>
                        <td></td>
